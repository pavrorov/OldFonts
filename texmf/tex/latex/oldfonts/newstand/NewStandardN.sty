%% Provides fonts scaling via the parameter [scaled=<factor>] 
%% in analogy to helvet.sty from psnfss bundle.
%%
\ProvidesPackage{NewStandardN}
   [2004/03/10 NewStandardOldNarrow Cyrillic font as default Roman]
\RequirePackage{keyval}
\define@key{Nsn}{scaled}[1.0]{%
  \def\Sn@scale{#1}}
\def\ProcessOptionsWithKV#1{%
  \let\@tempc\relax
  \let\Sn@tempa\@empty
  \@for\CurrentOption:=\@classoptionslist\do{%
    \@ifundefined{KV@#1@\CurrentOption}%
    {}%
    {\edef\Sn@tempa{\Sn@tempa,\CurrentOption,}}%
  }%
  \edef\Sn@tempa{%
    \noexpand\setkeys{#1}{%
      \Sn@tempa\@ptionlist{\@currname.\@currext}%
    }%
  }%
  \Sn@tempa
}
\ProcessOptionsWithKV{Nsn}
\AtEndOfPackage{%
  \let\@unprocessedoptions\relax
}
\renewcommand{\rmdefault}{fnn}
\endinput
