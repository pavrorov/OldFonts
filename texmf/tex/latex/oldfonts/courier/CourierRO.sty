\ProvidesPackage{CourierRO}[2005/05/31 CourierOR Cyrillic font as default roman]
\RequirePackage{keyval}
\define@key{Cro}{scaled}[.95]{%
  \def\Co@scale{#1}}
\def\ProcessOptionsWithKV#1{%
  \let\@tempc\relax
  \let\Co@tempa\@empty
  \@for\CurrentOption:=\@classoptionslist\do{%
    \@ifundefined{KV@#1@\CurrentOption}%
    {}%
    {\edef\Co@tempa{\Co@tempa,\CurrentOption,}}%
  }%
  \edef\Co@tempa{%
    \noexpand\setkeys{#1}{%
      \Co@tempa\@ptionlist{\@currname.\@currext}%
    }%
  }%
  \Co@tempa
}
\ProcessOptionsWithKV{Cro}
\AtEndOfPackage{%
  \let\@unprocessedoptions\relax
}
\renewcommand{\ttdefault}{tcr}
\endinput
